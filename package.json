{
  "name": "touhou-school-of-fish",
  "version": "0.3.1",
  "description": "東方Project楽曲のイントロクイズに向けた学習・演習用アプリ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --dir",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win",
    "license-check": "license-checker --production --failOn 'GPL;AGPL;LGPL'",
    "generate-licenses": "generate-license-file --input package.json --output THIRD-PARTY-LICENSES.txt --overwrite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "otagao",
  "license": "MIT",
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "generate-license-file": "^3.0.0",
    "license-checker": "^25.0.1"
  },
  "dependencies": {
    "howler": "^2.2.3",
    "papaparse": "^5.4.0"
  },
  "build": {
    "appId": "com.otagao.touhouschooloffish",
    "productName": "Touhou School of Fish",
    "directories": {
      "output": "dist"
    },
    "extraResources": [
      "LICENSE",
      "THIRD-PARTY-LICENSES.txt",
      "NOTICE"
    ],
    "files": [
      "main.js",
      "preload.js",
      "src/**/*",
      "node_modules/**/*",
      "package.json",
      "!node_modules/**/test/**",
      "!node_modules/**/*.md",
      "!node_modules/**/LICENSE*",
      "!node_modules/**/.npmignore"
    ],
    "asarUnpack": [],
    "fileAssociations": [],
    "electronLanguages": [
      "ja",
      "en-US"
    ],
    "afterPack": "./build-cleanup.js",
    "mac": {
      "category": "public.app-category.music",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "forceCodeSigning": false,
      "signAndEditExecutable": false,
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "AudioVideo"
    }
  }
}
